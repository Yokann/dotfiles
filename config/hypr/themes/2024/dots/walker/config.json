{
    "activation_mode": {
        "labels": "jkl:asdf"
    },
    "as_window": false,
    "builtins": {
        "applications": {
            "actions": {
                "enabled": false,
                "hide_category": false,
                "hide_without_query": false
            },
            "context_aware": true,
            "name": "applications",
            "placeholder": "Applications",
            "prioritize_new": false,
            "refresh": true,
            "cache": false,
            "show_generic": true,
            "show_icon_when_single": true,
            "show_sub_when_single": true,
            "weight": 6
        },
        "calc": {
            "icon": "accessories-calculator",
            "min_chars": 4,
            "name": "calc",
            "placeholder": "Calculator",
            "weight": 5
        },
        "clipboard": {
            "image_height": 300,
            "max_entries": 40,
            "name": "clipboard",
            "placeholder": "Clipboard",
            "switcher_only": true,
            "always_put_new_on_top": true,
            "avoid_line_breaks": true,
            "weight": 5
        },
        "commands": {
            "icon": "utilities-terminal",
            "name": "commands",
            "placeholder": "Commands",
            "switcher_only": true,
            "weight": 5
        },
        "custom_commands": {
            "icon": "utilities-terminal",
            "name": "custom_commands",
            "placeholder": "Custom Commands",
            "weight": 5
        },
        "dmenu": {
            "name": "dmenu",
            "placeholder": "Dmenu",
            "switcher_only": true,
            "weight": 5
        },
        "emojis": {
            "history": true,
            "name": "emojis",
            "placeholder": "Emojis",
            "switcher_only": true,
            "typeahead": true,
            "weight": 5
        },
        "finder": {
            "concurrency": 8,
            "icon": "folder",
            "ignore_gitignore": true,
            "name": "finder",
            "placeholder": "Finder",
            "refresh": true,
            "switcher_only": true,
            "weight": 5
        },
        "runner": {
            "generic_entry": false,
            "history": true,
            "icon": "utilities-terminal",
            "name": "runner",
            "placeholder": "Runner",
            "refresh": true,
            "typeahead": true,
            "switcher_only": true,
            "weight": 5
        },
        "ssh": {
            "history": true,
            "icon": "preferences-system-network",
            "name": "ssh",
            "placeholder": "SSH",
            "refresh": true,
            "switcher_only": true,
            "weight": 5
        },
        "switcher": {
            "name": "switcher",
            "placeholder": "Switcher",
            "prefix": "/",
            "weight": 5
        },
        "websearch": {
            "weight": 5,
            "icon": "applications-internet",
            "name": "websearch",
            "placeholder": "Search...",
            "prefix": "?",
            "entries": [
                {
                    "name": "Google",
                    "url": "https://www.google.com/search?q=%TERM%",
                    "prefix": "g"
                },
                {
                    "name": "Github",
                    "url": "https://github.com/search?q=%TERM%",
                    "prefix": "git"
                },
                {
                    "name": "Amazon",
                    "url": "https://www.amazon.fr/s?k=%TERM%",
                    "prefix": "a"
                },
                {
                    "name": "Repology",
                    "url": "https://repology.org/projects/?search=%TERM%",
                    "prefix": "repo"
                },
                {
                    "name": "HowLongToBeat",
                    "url": "https://howlongtobeat.com/?q=%TERM%",
                    "prefix": "hltb"
                }
            ]
        },
        "windows": {
            "icon": "view-restore",
            "name": "windows",
            "placeholder": "Windows",
            "prefix": "[]",
            "weight": 5
        }
    },
    "disabled": [
        "calc",
        "bookmarks",
        "ai"
    ],
    "plugins": [
        {
            "name": "twitch",
            "src": "node ~/.dotfiles/config/hypr/themes/2024/dots/walker/scripts/twitch.cjs",
            "placeholder": "Select a live stream",
            "show_sub_when_single": true,
            "show_icon_when_single": true,
            "switcher_only": true
        }
    ],
    "disable_click_to_close": false,
    "force_keyboard_focus": true,
    "list": {
        "max_entries": 50,
        "show_initial_entries": true,
        "single_click": true
    },
    "search": {
        "delay": 0,
        "force_keyboard_focus": true,
        "history": true,
        "placeholder": "Search..."
    },
    "hotreload_theme": true,
    "theme": "catppuccin"
}
